{
  "name": "chopsticks",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bot": "STORAGE_DRIVER=postgres node --inspect=0.0.0.0:9229 src/index.js",
    "agents": "node src/agents/agentRunner.js",
    "docker:lavalink": "docker compose -f docker-compose.lavalink.yml up -d",
    "start": "npm run docker:lavalink && npm run bot",
    "start:all": "concurrently --raw --prefix \"[{name}]\" --prefix-colors \"auto\" -n main,agents \"AUTO_START_AGENTS=false npm run bot\" \"npm run agents\"",
    "start:shard": "node src/shard.js",
    "dashboard": "node src/dashboard/server.js",
    "migrate": "node scripts/migrate.js",
    "seed:pg": "node scripts/seedFromFiles.js",
    "deploy:guild": "DEPLOY_MODE=guild node scripts/deployCommands.js",
    "deploy:global": "DEPLOY_MODE=global node scripts/deployCommands.js",
    "clear:guild": "CLEAR_MODE=guild node scripts/clearCommands.js",
    "clear:global": "CLEAR_MODE=global node scripts/clearCommands.js"
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "async": "^3.2.6",
    "connect-redis": "^8.0.1",
    "discord.js": "^14.16.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "lavalink-client": "^2.7.7",
    "opusscript": "^0.0.8",
    "pg": "^8.11.5",
    "prism-media": "^1.3.5",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "undici": "^6.23.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "overrides": {
    "undici": "^6.23.0"
  }
}
