{
  "name": "chopsticks",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bot": "STORAGE_DRIVER=postgres node --inspect=0.0.0.0:9229 src/index.js",
    "agents": "node src/agents/agentRunner.js",
    "docker:lavalink": "docker compose -f docker-compose.lavalink.yml up -d",
    "start": "npm run docker:lavalink && npm run bot",
    "start:all": "concurrently --raw --prefix \"[{name}]\" --prefix-colors \"auto\" -n main,agents \"AUTO_START_AGENTS=false npm run bot\" \"npm run agents\"",
    "start:shard": "node src/shard.js",
    "dashboard": "node src/dashboard/server.js",
    "funhub": "node src/fun/server.js",
    "migrate": "node scripts/migrate.js",
    "seed:pg": "node scripts/seedFromFiles.js",
    "deploy:guild": "DEPLOY_MODE=guild node scripts/deployCommands.js",
    "deploy:global": "DEPLOY_MODE=global node scripts/deployCommands.js",
    "clear:guild": "CLEAR_MODE=guild node scripts/clearCommands.js",
    "clear:global": "CLEAR_MODE=global node scripts/clearCommands.js",
    "monitoring:up": "docker compose -f docker-compose.monitoring.yml up -d",
    "monitoring:down": "docker compose -f docker-compose.monitoring.yml down",
    "monitoring:logs": "docker compose -f docker-compose.monitoring.yml logs -f",
    "test": "mocha 'test/unit/**/*.test.js'",
    "test:watch": "mocha 'test/unit/**/*.test.js' --watch",
    "test:level-1": "mocha 'test/unit/protocol-version.test.js' 'test/unit/agent-limits.test.js' 'test/unit/agent-deployment.test.js' 'test/unit/session-lifecycle.test.js'"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@sentry/node": "^10.38.0",
    "@snazzah/davey": "^0.1.9",
    "async": "^3.2.6",
    "autocannon": "^8.0.0",
    "bcrypt": "^6.0.0",
    "clinic": "^13.0.0",
    "compression": "^1.8.1",
    "connect-redis": "^8.0.1",
    "cors": "^2.8.6",
    "discord.js": "^14.16.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.9.3",
    "joi": "^18.0.2",
    "lavalink-client": "^2.7.7",
    "opusscript": "^0.0.8",
    "pg": "^8.11.5",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "prism-media": "^1.3.5",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^9.1.1",
    "redis": "^4.7.0",
    "sodium-native": "^5.0.10",
    "undici": "^6.23.0",
    "opossum": "^8.1.0",
    "async-retry": "^1.3.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "mocha": "^11.7.5"
  },
  "overrides": {
    "undici": "^6.23.0"
  }
}
